<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kunxin.mobile.android.kxscs.activity.ShipCompanyActivity">
    <include layout="@layout/title_layout"/>
    <com.wl.view.MySearchView
         android:id="@+id/activity_select_ship_company"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"/>
    <ListView
        android:id="@+id/ship_company_list"
        android:layout_below="@id/activity_select_ship_company"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/margin_10"
        android:layout_marginLeft="@dimen/margin_10"/>
</LinearLayout>
